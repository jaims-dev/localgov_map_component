<?php

/**
 * @file
 * Primary module hooks for Localgov Map Component module.
 */

/**
 * Implements hook_theme().
 *
 * Use a module-provided template.
 */
function localgov_map_component_theme($existing, $type, $theme, $path) {
  return [
    'paragraph__map' => [
      'template' => 'paragraph--map',
      'base hook' => 'paragraph',
      'path' => $path . '/templates',
    ],
  ];
}
